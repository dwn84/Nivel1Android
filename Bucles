package miProgramita;

public class Bucles {

	public static void main(String[] args) {
		
		int a = 5;
		int b = a + 10;
		System.out.println("Inicio del programa");
		for (int i = 0;i<10;i++){
			System.out.println("Hola");
			a++;			
			b = a + b;
			System.out.println("Hoy es un buen dÃ­a");			
		}
		System.out.println("Fin del programa");

	}

}
